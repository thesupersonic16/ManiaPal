<UserControl x:Class="ManiaPal.Extensions.GGPAL.FileOptionsE"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ManiaPal.Extensions.GGPAL"
             mc:Ignorable="d" 
             d:DesignHeight="224" d:DesignWidth="538">
    <Grid>
        <GroupBox Header="Import/Export Settings" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <Grid>
                <Label Content="Palette Format:" HorizontalAlignment="Left" Margin="10,8,0,0" VerticalAlignment="Top"/>
                <ComboBox x:Name="UI_FormatComboBox" SelectedIndex="0" HorizontalAlignment="Left" Height="22" VerticalAlignment="Top" Width="227" Margin="104,12,0,0">
                    <ComboBoxItem Content="JASC (for Graphics Gale)"/>
                    <ComboBoxItem Content="Microsoft Palette"/>
                </ComboBox>
            </Grid>
        </GroupBox>
    </Grid>
</UserControl>
